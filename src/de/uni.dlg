                        NOT WS_VISIBLE | WS_DISABLED
        GROUPBOX        "Number of copies", GB_COPIES, 230, 43, 5, 26, NOT 
                        WS_VISIBLE
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "9.WarpSans Bold"
        LTEXT           "Spool this job", DT_COPIES, 230, 33, 7, 8, 
                        DT_VCENTER | NOT WS_VISIBLE
        LTEXT           "times to the server", DT_COPIES2, 229, 23, 7, 8, 
                        DT_VCENTER | NOT WS_VISIBLE
        ENTRYFIELD      "", ID_COPIES, 101, 7, 23, 8, ES_MARGIN | NOT 
                        WS_VISIBLE
    END
END
