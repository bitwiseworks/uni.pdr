#ifndef OS2_INCLUDED
   #include <os2.h>
#endif
DLGINCLUDE 1 "uni.h"

DLGTEMPLATE IDD_UNI LOADONCALL MOVEABLE DISCARDABLE
BEGIN
    DIALOG  "Port Properties - ", IDD_UNI, 19, 122, 237, 71, FS_NOBYTEALIGN | 
            WS_VISIBLE | WS_SYNCPAINT, FCF_SYSMENU | FCF_TITLEBAR | 
            FCF_NOBYTEALIGN
            PRESPARAMS PP_FONTNAMESIZE, "9.WarpSans"
    BEGIN
        DEFPUSHBUTTON   "OK", DID_OK, 5, 5, 76, 14
                        PRESPARAMS PP_FONTNAMESIZE, "9.WarpSans Bold"
        PUSHBUTTON      "Cancel ", DID_CANCEL, 150, 5, 76, 14
                        PRESPARAMS PP_FONTNAMESIZE, "9.WarpSans Bold"
        GROUPBOX        "Universal port driver properties", GB_UNICFG, 5, 23, 
                        220, 42
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "9.WarpSans Bold"
        LTEXT           "Program:", DT_PROGRAM, 13, 45, 53, 8, DT_VCENTER
        ENTRYFIELD      "", ID_PROGRAM, 71, 46, 142, 8, ES_MARGIN
        LTEXT           "Parameters:", DT_PARAMETERS, 13, 31, 53, 8, 
                        DT_VCENTER
        ENTRYFIELD      "", ID_PARAMETERS, 71, 32, 142, 8, ES_MARGIN
